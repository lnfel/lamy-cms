		"prettier": "2.8.7",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"typescript": "^5.0.0",
		"vite": "^4.2.0"
	},
	"type": "module",
	"dependencies": {
		"@tauri-apps/api": "^1.2.0",
		"@twind/core": "^1.1.3",
		"@twind/preset-autoprefix": "^1.0.7",
		"@twind/preset-tailwind": "^1.1.4",
		"@twind/with-sveltekit": "^1.1.3"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowedVersions": {
				"@twind/core>typescript": "5",
